{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "semi": "off",
        "linebreak-style": "off",
        "arrow-parens": "off",
        "indent": "off",
        "comma-dangle": "off",
        "quotes": "off",
        "eol-last": "off",
        "require-jsdoc": "off",
        "no-trailing-spaces": "off",
        "object-curly-spacing": "off",
        "no-unused-vars": "off",
        "operator-linebreak": "off",
        "no-debugger": "off",
        "spaced-comment": "off",
        "padded-blocks": "off",
        "max-len": "off",
        "no-undef": "off",
        "guard-for-in": "off",
        "prefer-const": "off",
        "no-prototype-builtins": "off",
        "no-case-declarations": "off"
    },
    "extends": ["eslint:recommended", "google"]
}
